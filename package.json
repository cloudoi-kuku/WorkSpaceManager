{
  "name": "manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:all": "concurrently \"npm run dev\" \"cd cms-backend && npm run dev\" --names \"FRONTEND,PAYLOAD\" --prefix-colors \"green,yellow\"",
    "dev:stop": "killall node",
    "payload:migrate": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts tsx src/payload/migrate.ts",
    "payload:generate-types": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts tsx src/payload/generate-types.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@payloadcms/db-postgres": "^3.35.1",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "lucide-react": "^0.503.0",
    "payload": "^3.35.1",
    "pg": "^8.15.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.1",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/pg": "^8.11.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.19.3",
    "tw-animate-css": "^1.2.8",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
